{
  "name": "@degenie/backend",
  "version": "1.0.0",
  "description": "DeGenie Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/complete-server.ts",
    "dev:main": "npm run dev -- src/index.ts",
    "dev:simple": "npm run dev -- src/simple-server.ts",
    "dev:test": "npm run dev -- src/test-server.ts",
    "dev:web": "npm run dev -- src/web-interface.ts",
    "dev:complete": "npm run dev -- src/complete-server.ts",
    "dev:working": "npm run dev -- src/working-server.ts",
    "dev:final": "npm run dev -- src/final-server.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:quality": "ts-node src/tests/run-quality-tests.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@metaplex-foundation/js": "^0.20.1",
    "@prisma/client": "^5.7.0",
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.2",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "ipfs-http-client": "^60.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "replicate": "^0.25.1",
    "socket.io": "^4.7.2",
    "together-ai": "^0.5.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
